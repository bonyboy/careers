        <div id="editor-content">
            <div id="formWrapper">


                <div runat="server" id="DivMain2">

                <form runat="server" class="cmxform" id="lead_new">

                <asp:HiddenField runat="server" ID="HiddenFieldJobId" Visible="false" />
                <asp:HiddenField runat="server" ID="HiddenFieldFileNames" Visible="false" />
                <asp:HiddenField runat="server" ID="HiddenFieldUserIP" Visible="false" />

                <p>Please complete the form below. Mandatory fields marked <em>*</em></p>

                <fieldset>
                <ol>
                    <li class="lead_line_li_wrapper first_name required string_label">
                        <label class="lead_form_label required string_label" for="lead_first_name" id="first_name_label">Name<em>*</em></label>
                    
                        <asp:TextBox runat="server" ID="TextBoxName" MaxLength="50" ValidationGroup="vg" />

                        <asp:RequiredFieldValidator runat="server" id="RFVName" ControlToValidate="TextBoxName" SetFocusOnError="true"
                        ErrorMessage="Name field is required">*</asp:RequiredFieldValidator>
                    
                    </li>
                    <li class="lead_line_li_wrapper email required string_label">
                        <label class="lead_form_label required string_label" for="lead_email" id="email_label">Email<em>*</em></label>

                        <asp:TextBox runat="server" ID="TextBoxEmail" MaxLength="50" ValidationGroup="vg" />
    
                        <asp:RequiredFieldValidator runat="server" id="RFVEmail" ControlToValidate="TextBoxEmail" SetFocusOnError="true"
                        ErrorMessage="Email field is required" Display="Dynamic">*</asp:RequiredFieldValidator>

                        <asp:RegularExpressionValidator id="REVEmail" runat="server" ControlToValidate="TextBoxEmail"
                        ValidationExpression="[\040\t]*\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*[\040\t]*" SetFocusOnError="true"
                        ErrorMessage="Email is invalid" Display="Dynamic">*</asp:RegularExpressionValidator>

                    </li>
                    <li class="lead_line_li_wrapper telephone string_label">
                        <label class="lead_form_label string_label" for="lead_telephone" id="telephone_label">Phone</label>

                        <asp:TextBox runat="server" ID="TextBoxPhone" MaxLength="50" />

                    </li>
                    <li class="lead_line_li_wrapper string_label">
                        <label class="lead_form_label string_label" for="file_label" id="file_label">Resume</label>

                        <input type="file" runat="server" ID="FileUpload1" />

                    </li>
                    <li class="lead_line_li_wrapper string_label">
                        <label class="lead_form_label string_label" for="file_label" id="Label1">Cover Letter</label>

                        <input type="file" runat="server" ID="FileUpload2" />

                    </li>
                    <li class="lead_line_li_wrapper string_label">
                        <label class="lead_form_label string_label" for="file_label" id="Label2"></label>

                        Maximum total file size is <b>20 megabytes</b>.
                   
                    </li>
                    <li class="lead_line_li_wrapper message text_label">
                        <label class="lead_form_label  text_label" for="lead_message" id="message_label">Message</label>

                        <asp:TextBox runat="server" ID="TextBoxMessage" TextMode="MultiLine" ValidationGroup="vg" style="height:110px;" />

                        <asp:RegularExpressionValidator runat="server" id="REVMessage" SetFocusOnError="true" ControlToValidate="TextBoxMessage"
                        ValidationExpression="[\s\S]{0,2000}$" ErrorMessage="Message character limit is 2000">*</asp:RegularExpressionValidator>

                    </li>
                </ol>
                </fieldset>
    
                <cb:CustomButton runat="server" ID="ButtonSubmit" UseSubmitBehavior="false" Text="Submit" IsDisable="true" OnClick="ButtonSubmit_Click" />

                <asp:ValidationSummary ID="VS1" runat="server" ShowMessageBox="True" ShowSummary="False" ValidationGroup="vg"
                HeaderText="You must correct the following fields:" />
                   
                </form>
